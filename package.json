{
  "name": "drykiss.com",
  "license": "ISC",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "check": "npm-check -s -u",
    "start": "node .",
    "build": "next build",
    "export": "next export",
    "package": "yarn run build && yarn run export",
    "postexport": "yarn run babel-node --ignore=node_modules ./scripts/postExport.js",
    "production": "push-dir --dir=out --remote=git@github.com:DryKISS/drykiss.com.git --branch=gh-pages",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "serve": "serve out"
  },
  "standard": {
    "globals": [
      "__SITE__"
    ],
    "ignore": [
      "node_modules/**",
      "*.bundle*.js"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/pro-regular-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "firebase": "^6.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.0.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.8",
    "react-select": "^3.0.4",
    "react-tooltip": "^3.11.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.6.1",
    "@zeit/next-css": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "next-transpile-modules": "^2.3.1",
    "npm-check": "^5.9.0",
    "push-dir": "^0.4.1",
    "serve": "^11.1.0",
    "sitemap": "^4.1.1",
    "standard": "^14.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
