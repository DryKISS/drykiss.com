{
  "name": "drykiss.com",
  "private": true,
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "start": "next",
    "lint": "standard | snazzy",
    "lint-fix": "standard --fix --verbose | snazzy",
    "prettier": "prettier-standard './**/*.js'",
    "stylelint": "stylelint './**/*.js'",
    "test": "yarn run lint-fix && yarn run prettier && yarn run stylelint",
    "build": "next build",
    "export": "next export",
    "package": "yarn run build && yarn run export",
    "postexport": "node ./scripts/postExport.js",
    "production": "push-dir --dir=out --remote=git@github.com:DryKISS/drykiss.com.git --branch=gh-pages",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "serve": "serve out"
  },
  "engines": {
    "node": ">10.15.0"
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "standard": {
    "globals": ["__SITE__", "cy", "describe", "it"],
    "ignore": ["node_modules/**", "*.bundle*.js"],
    "parser": "babel-eslint"
  },
  "stylelint": {
    "processors": ["stylelint-processor-styled-components"],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier",
      "stylelint-config-styled-components"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.0.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/pro-duotone-svg-icons": "^5.13.0",
    "@fortawesome/pro-regular-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@tanem/react-nprogress": "^3.0.27",
    "@tippyjs/react": "^4.1.0",
    "axios": "^0.19.2",
    "graphql-tag": "^2.10.3",
    "next": "^9.3.3",
    "polished": "^3.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.10",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@nivo/bar": "^0.61.1",
    "@nivo/generators": "^0.61.0",
    "@nivo/line": "^0.61.1",
    "@nivo/pie": "^0.61.1",
    "@typeform/embed": "^0.16.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "bcryptjs": "^2.4.3",
    "emoji-mart": "^3.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "graphql": "^14.6.0",
    "gravatar": "^1.8.0",
    "holderjs": "^2.9.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prettier-standard": "^16.2.1",
    "push-dir": "^0.4.1",
    "react-datepicker": "^2.14.1",
    "react-dropzone": "^10.2.2",
    "react-dropzone-uploader": "^2.11.0",
    "react-google-maps": "^9.4.5",
    "react-hook-form": "^5.2.0",
    "react-select": "^3.1.0",
    "serve": "^11.3.0",
    "sitemap": "^6.1.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.3",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "yup": "^0.28.3"
  }
}
