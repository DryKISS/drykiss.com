{
  "name": "drykiss.com",
  "private": true,
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "check": "npm-check -s -u",
    "start": "next",
    "lint": "standard | snazzy",
    "lint-fix": "standard --fix --verbose | snazzy",
    "prettier": "prettier-standard './**/*.js'",
    "stylelint": "stylelint './**/*.js'",
    "test": "yarn run lint-fix && yarn run prettier && yarn run stylelint",
    "build": "next build",
    "export": "next export",
    "package": "yarn run build && yarn run export",
    "postexport": "node ./scripts/postExport.js",
    "production": "push-dir --dir=out --remote=git@github.com:DryKISS/drykiss.com.git --branch=gh-pages",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "serve": "serve out"
  },
  "engines": {
    "node": ">10.15.0"
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "standard": {
    "globals": ["__SITE__", "cy", "describe", "it"],
    "ignore": ["node_modules/**", "*.bundle*.js"],
    "parser": "babel-eslint"
  },
  "stylelint": {
    "processors": ["stylelint-processor-styled-components"],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier",
      "stylelint-config-styled-components"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/pro-regular-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.8.1",
    "react-hook-form-input": "^1.1.10",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/pro-duotone-svg-icons": "^5.12.0",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@nivo/bar": "^0.61.1",
    "@nivo/line": "^0.61.1",
    "@nivo/pie": "^0.61.1",
    "@nivo/generators": "^0.61.0",
    "@typeform/embed": "^0.15.3",
    "@zeit/next-css": "^1.0.1",
    "apollo-client": "^2.6.8",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "bcryptjs": "^2.4.3",
    "emoji-mart": "^2.11.2",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "graphql": "^14.6.0",
    "holderjs": "^2.9.6",
    "jsonwebtoken": "^8.5.1",
    "next-transpile-modules": "^3.0.2",
    "npm-check": "^5.9.0",
    "prettier-standard": "^16.1.0",
    "push-dir": "^0.4.1",
    "react-datepicker": "^2.11.0",
    "react-dropzone": "^10.2.1",
    "react-google-maps": "^9.4.5",
    "react-select": "^3.0.8",
    "react-tooltip": "^3.11.3",
    "serve": "^11.3.0",
    "sitemap": "^5.1.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0"
  }
}
