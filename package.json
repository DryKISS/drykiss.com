{
  "name": "drykiss.com",
  "private": true,
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "check": "npm-check -s -u",
    "start": "next",
    "lint": "standard | snazzy",
    "lint-fix": "standard --fix --verbose | snazzy",
    "prettier": "prettier-standard './**/*.js'",
    "stylelint": "stylelint './**/*.js'",
    "build": "next build",
    "export": "next export",
    "package": "yarn run build && yarn run export",
    "postexport": "node ./scripts/postExport.js",
    "production": "push-dir --dir=out --remote=git@github.com:DryKISS/drykiss.com.git --branch=gh-pages",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "serve": "serve out"
  },
  "engines": {
    "node": ">10.15.0"
  },
  "eslint": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "standard": {
    "globals": [
      "__SITE__",
      "cy",
      "describe",
      "it"
    ],
    "ignore": [
      "node_modules/**",
      "*.bundle*.js"
    ],
    "parser": "babel-eslint"
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier",
      "stylelint-config-styled-components"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/pro-regular-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "next": "^9.1.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@fortawesome/pro-duotone-svg-icons": "^5.11.2",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@nivo/bar": "^0.59.2",
    "@nivo/generators": "^0.59.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "bcryptjs": "^2.4.3",
    "emoji-mart": "^2.11.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "next-transpile-modules": "^2.3.1",
    "npm-check": "^5.9.0",
    "push-dir": "^0.4.1",
    "prettier-standard": "^15.0.1",
    "react-dropzone": "^10.1.10",
    "react-select": "^3.0.8",
    "react-tooltip": "^3.11.1",
    "serve": "^11.2.0",
    "sitemap": "^5.0.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
